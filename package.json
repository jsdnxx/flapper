{
  "name": "flapper",
  "version": "0.0.0-POC",
  "description": "flow automatic property prover",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:build && npm run test:run",
    "test:run": "mocha obj/**/*.test.js",
    "test:build": "babel src --out-dir obj",
    "test:watch": "npm run test:watch:build & npm run test:watch:run",
    "test:watch:build": "babel src --out-dir obj --watch",
    "test:watch:run": "mocha obj/**/*.test.js --watch"
  },
  "author": "https://npmjs.com/~jsdnxx",
  "repository": "jsdnxx/flapper",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-es2015": "^1.2.0",
    "chai": "^3.5.0",
    "flow-bin": "^0.37.0",
    "mocha": "^3.2.0",
    "standard": "^8.6.0",
    "webpack": "^1.14.0"
  },
  "standard": {
    "ignore": [
      "flow-typed/*"
    ],
    "parser": "babel-eslint"
  }
}
